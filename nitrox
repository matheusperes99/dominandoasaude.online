<body>
    <p class="counter">
        <span class="timer">453</span>&nbsp;pessoas assistindo agora...
    </p>

    <script>
        // Inicialize o contador com o valor inicial
        let count = 453;

        // Função para incrementar o contador
        function incrementCounter() {
            count++;
            document.querySelector('.timer').textContent = count;
        }

        // Incrementa o contador a cada 5 segundos (5000 milissegundos)
        setInterval(incrementCounter, 5000);
    </script>
</body>
