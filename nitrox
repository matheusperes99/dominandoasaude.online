<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Esconder Conteúdo por 15 Minutos</title>
    <style>
        #conteudoEscondido {
            display: none;
        }
    </style>
    <script>
        window.onload = function() {
            // Tempo em milissegundos (15 minutos = 900000 milissegundos)
            var tempoEspera = 1140000;

            // Função para mostrar o conteúdo
            function mostrarConteudo() {
                document.getElementById('conteudoEscondido').style.display = 'block';
            }

            // Esconde o conteúdo inicialmente
            document.getElementById('conteudoEscondido').style.display = 'none';

            // Define um temporizador para mostrar o conteúdo após o tempo especificado
            setTimeout(mostrarConteudo, tempoEspera);
        };
    </script>
</head>
